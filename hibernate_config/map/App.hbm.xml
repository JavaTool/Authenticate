<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.authenticate.app">
    <class name="App" table="app">
        <id name="appId">
            <generator class="assigned"/>
        </id>
        <property name="appKey" type="string"/>
        <property name="appName" type="string"/>
        <property name="appTable" type="string"/>
        <property name="appEntry" type="string"/>
    </class>
</hibernate-mapping>